<!DOCTYPE html>
<html lang="en">
  <%- include("./partials/head") %>
  <body>
    <%- include("./partials/nav") %>
    <main class="blogs content">
      <h2>All Blogs</h2>
      <% blogs.forEach(blog => { %>
      <article>
        <a href="/blog/<%= blog._id %>">
          <h3><%= blog.title %></h3>
        </a>
        <p><%= blog.snippet %></p>
      </article>
      <% }) %>
    </main>
    <%- include("./partials/footer") %>
  </body>
</html>
